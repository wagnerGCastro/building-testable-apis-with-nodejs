{
  "name": "node-book",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:babel": "babel src --out-dir dist",
    "build": "npx sucrase ./src -d ./dist --transforms imports",
    "start": "npm run build && node dist/server.js",
    "start:dev": "cross-env NODE_ENV=development nodemon src/server.js",
    "start:stage": "cross-env NODE_ENV=staging nodemon src/server.js",
    "test:integration": "cross-env NODE_ENV=test mocha --opts test/integration/mocha.opts test/integration/**/*_spec.js",
    "test:unit": "cross-env NODE_ENV=test mocha --opts test/unit/mocha.opts test/unit/**/*_spec.js",
    "test:mocha": "npm run test:unit && npm run test:integration",
    "test": "jest",
    "test:cover": "jest --coverage",
    "lint": "eslint src --ext .js",
    "lint:fix": "eslint src --fix --ext .js",
    "prettier:list": "prettier --check 'src/**/*.js'",
    "prettier:fix": "prettier --write 'src/**/*.js'",
    "prettier-format": "npx prettier --write '*.{js,ts,md,json}' --config ./.prettierrc",
    "style:fix": "npm run lint:fix & npm run prettier:fix"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@types/jest": "^29.4.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^29.5.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^29.5.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "sinon": "^7.5.0",
    "sucrase": "^3.29.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.15.2",
    "config": "^1.29.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.14.0",
    "express-acl": "^2.0.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.7.13"
  }
}
